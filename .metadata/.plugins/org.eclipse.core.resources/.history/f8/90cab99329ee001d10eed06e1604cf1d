package com.spring.basic;

public class LgTv implements Lg {
	@Override
	public void turnOn() {
		System.out.println("LGTV - 전원 on");
	}
	@Override
	public void turnOff() {
		System.out.println("LGTV - 전원 off");
	}
	@Override
	public void soundUp() {
		System.out.println("LGTV - 볼륨 Up");
	}
	@Override
	public void soundDown() {
		System.out.println("LGTV - 볼륨 Down");
	}
}
